{
  "name": "ahj-http",
  "version": "1.0.0",
  "description": "http",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "test": "jest --testPathPattern=./src/",
    "coverage": "jest --testPathPattern=./src/ --coverage",
    "e2e": "jest --testPathPattern=./e2e/ --runInBand --detectOpenHandles",
    "doc": "jsdoc -c jsdoc.conf.json",
    "preshow:coverage": "npm run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:doc": "npm run doc",
    "show:doc": "live-server doc",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayostar/ahj-http.git"
  },
  "author": "Artem Starodubtsev <ayostar@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ayostar/ahj-http/issues"
  },
  "homepage": "https://github.com/ayostar/ahj-http#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "core-js": "^3.26.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^4.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^29.3.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.3.3",
    "puppeteer": "^19.3.0",
    "push-dir": "^0.4.1",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  }
}
